{
  "name": "jron82/playground",
  "description": "A playground to, well, play",
  "keywords": [
    "playground",
    "fun",
    "experiment"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Ronald Jordan",
      "email": "ron@rdev.co.za"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "symfony/console": "^4.2",
    "vlucas/phpdotenv": "^3.3",
    "webmozart/assert": "^1.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.10.7",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Playground\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
